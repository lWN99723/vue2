     <h1>{{item.title}}</h1>\r\n                        <p class='mui-ellipsis'>\r\n                            <span>{{item.add_time | datetimeFilter}}</span>\r\n                            <span>点击:{{item.click}}次</span>\r\n                        </p>\r\n                    </div>\r\n                </router-link>\r\n            </li>\r\n       </ul>\r\n    </div>\r\n</template>\r\n<script>\r\n    import {Toast} from \"mint-ui\";\r\n    export default{\r\n        name:\"NewsList\",\r\n        data(){\r\n            return{\r\n                newslist:[]\r\n            }\r\n        },\r\n        created(){\r\n            this.getNewsList();\r\n        },\r\n        methods:{\r\n            getNewsList(){\r\n                this.$http.get(\"newslist/list\").then(result=>{\r\n                    var obj=result.body;\r\n                    if(obj.code==1){   //返回成功数据\r\n                        this.newslist=obj.msg;\r\n                    }else{   //返回失败数据\r\n                        //测试 this.$toast(\"数据加载失败\")\r\n                        Toast(\"数据加载失败\")\r\n                    }\r\n                })\r\n            }\r\n        }\r\n    }\r\n</script>\r\n<style>\r\n    .mui-table-view h1{\r\n        font-size:14px;\r\n    }\r\n    .mui-table-view .mui-ellipsis{\r\n        font-size:12px;\r\n        color:#007aff;\r\n        display:flex;\r\n        justify-content: space-between;\r\n    }\r\n    .mui-table-view .mui-table-view-cell img{\r\n        width:25px;\r\n        height:25px;\r\n        margin-top:10px;\r\n    }\r\n</style>"]}]}