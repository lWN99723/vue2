\r\n        <div class=\"mui-card\">\r\n            <div class=\"mui-card-header\">{{goodsInfo.lname}}</div>\r\n            <div class=\"mui-card-content\">\r\n                <div class=\"mui-card-content-inner\">\r\n                    <p>\r\n                        市场价: <del>¥{{goodsInfo.price_old}}</del>\r\n                        销售价: <span class=\"now_price\">¥{{goodsInfo.price_new}}</span>\r\n                    </p>\r\n                    <p>购买数量:</p>\r\n                    <div class=\"mui-numbox\">\r\n                        <button class=\"mui-btn mui-btn-numbox-minus\" type=\"button\">-</button>\r\n                        <input class=\"mui-input-numbox\" type=\"number\"  value=\"1\"/>\r\n                        <button class=\"mui-btn mui-btn-numbox-plus\" type=\"button\">+</button>\r\n                    </div>\r\n                    <p style=\"margin-top:10px\">\r\n                        <mt-button type=\"primary\">立即购买</mt-button>\r\n                        <mt-button type=\"danger\" @click=\"addCart\">加入购物车</mt-button>\r\n                    </p>\r\n                </div>\r\n            </div>\r\n            <div class=\"mui-card-footer\">页脚</div>\r\n        </div>\r\n        <!--商品参数区域-->\r\n        <div class=\"mui-card\">\r\n            <div class=\"mui-card-header\">页眉</div>\r\n            <div class=\"mui-card-content\">\r\n                <div class=\"mui-card-content-inner\">\r\n                    参数\r\n                </div>\r\n            </div>\r\n            <div class=\"mui-card-footer\">页脚</div>\r\n        </div>\r\n    </div>\r\n</template>\r\n<script>\r\n    //导入图片轮播组件\r\n    import swiper from \"../subcomponents/swiper.vue\";\r\n    export default{\r\n        data(){return {\r\n            imagelist:[],\r\n            id:this.$route.params.id,\r\n            goodsInfo:{},\r\n        }\r\n        },\r\n        created(){\r\n            this.getImageList();\r\n            this.getGoodsInfo();\r\n        },\r\n        methods:{\r\n            addCart(){\r\n                //调用vuex实例对象操作共享数据方法\r\n                this.$store.commit(\"increment\");\r\n                //查询购物车中共享数据值\r\n                console.log(this.$store.state.count);\r\n            },\r\n            getImageList(){\r\n                this.$http.get(\"imagelist/list\").then(result=>{\r\n                    if(result.body.code==1){   //返回成功数据\r\n                        this.imagelist=result.body.msg;\r\n                    }\r\n                })\r\n            },\r\n            getGoodsInfo(){\r\n                var id=this.id;\r\n                this.$http.get(\"goodslist/find?id=\"+id).then(result=>{\r\n                    if (result.body.code==1){\r\n                        this.goodsInfo=result.body.msg[0];\r\n                        //console.log(this.goodsInfo);\r\n                    }\r\n                })\r\n            },\r\n        },\r\n        //注册一个子组件\r\n        components:{\r\n            \"swiper-box\":swiper\r\n        }\r\n    }\r\n</script>\r\n<style>\r\n    .goods_info{\r\n        background-color: #eee;\r\n        overflow: hidden;\r\n    }\r\n    .goods_info .now_price{\r\n        color:red;\r\n        font-size: 16px;\r\n        font-weight: bold;\r\n    }\r\n</style>"]}]}