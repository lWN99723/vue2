\n            <span>点击:{{newsinfo.click}}次</span>\r\n        </p>\r\n        <!--内容-->\r\n        <div class=\"container\" v-html=\"newsinfo.content\">\r\n        </div>\r\n        <comment-box :id=\"this.id\"></comment-box>\r\n    </div>\r\n</template>\r\n<script>\r\n    //引入提示框\r\n    import {Toast} from \"mint-ui\";\r\n    //引入子组件\r\n    import comment from \"../subcomponents/comment.vue\"\r\n    export default{\r\n        name:\"NewsInfo\",\r\n        data(){\r\n            return{\r\n                newsinfo:{}, //创建空对象保存数据\r\n                id:this.$route.params.id  //新闻列表编号\r\n            }\r\n        },\r\n        created(){\r\n            // console.log(this.$route.params.id);\r\n            // console.log (this);\r\n            this.getNewsInfo();\r\n        },\r\n        methods:{\r\n            getNewsInfo(){\r\n                this.$http.get(\"newslist/detail?id=\"+this.id).then(result=>{\r\n                    //console.log(result);\r\n                    if(result.body.code==1){\r\n                        this.newsinfo=result.body.msg[0];\r\n                    }else{\r\n                        Toast(\"数据加载失败\");\r\n                    }\r\n                })\r\n            }\r\n        },\r\n        components:{ //newsInfo.vue注册子组件\r\n            'comment-box':comment  //组件名称:组件对象\r\n        }\r\n    }\r\n</script>\r\n<style>\r\n    .newsinfo-container{\r\n        padding:0 4px;\r\n    }\r\n    .newsinfo-container .title{\r\n        font-size: 16px;\r\n        text-align:center;\r\n        margin: 15px 0;\r\n        color:red;\r\n    }\r\n    .newsinfo-container .subtitle{\r\n        font-size: 13px;\r\n        color:#007aff;\r\n        display:flex;\r\n        justify-content: space-between;\r\n    }\r\n    .newsinfo-container .container img{\r\n        width:100%;\r\n    }\r\n</style>"]}]}